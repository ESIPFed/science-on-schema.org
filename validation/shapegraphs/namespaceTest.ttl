SOgood:
	sh:declare [
		sh:prefix "SOgood" ;
		sh:namespace <http://schema.org/> ;
	] .
	
SObad1:
	sh:declare [
		sh:prefix "SOgood" ;
		sh:namespace <https://schema.org/> ;
	] .
	
SObad2:
	sh:declare [
		sh:prefix "SOgood" ;
		sh:namespace <https://schema.org> ;
	] .

soso:DatasetNS1Shape
    a sh:NodeShape ;
    sh:target [
		a sh:SPARQLTarget ;
		sh:prefixes ex: ;
		sh:select """
			SELECT ?this
			WHERE {
				?this a ex:Person .
				?this ex:bornIn ex:USA .
			}
			""" ;
	] ;
	
	
