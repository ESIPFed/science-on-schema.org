  sh:datatype xsd:string ;
  sh:description "If this NewsArticle appears in print, this field indicates the name of the page on which the article is found. Please note that this field is intended for the exact page name (e.g. A5, B18)."^^rdf:HTML ;
  sh:name "printPage" ;
.
