  owl:equivalentClass <http://www.w3.org/ns/dcat#Dataset> ;
  sh:property [
      sh:path schema:catalog ;
      sh:class schema:DataCatalog ;
      sh:description "A data catalog which contains this dataset."^^rdf:HTML ;
